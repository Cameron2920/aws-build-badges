{
  "compilerOptions": {
    "noImplicitAny": false,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "es5",
    "outDir": "dist/",
    "moduleResolution": "node",
    "lib": [
      "es2015"
    ]
  },
  "exclude": [
    "src/timestamp-badges",
    "src/status-badges",
    "**/*.test.ts",
    "node_modules",
    "layers",
    "dist",
    ".vscode",
    ".serverless"
  ]
}